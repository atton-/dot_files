# global snippets

snippet     date_stamp
alias       ds
options     word
    `strftime("%Y/%m/%d(%a)")`

snippet     time_stamp
alias       ts
options     word
    `strftime("%Y/%m/%d %H:%M:%S")`

snippet     fold
options     word
	{{{ ${1}
	${2}
	}}}

snippet     line
options     word
    --------------------------------------------------

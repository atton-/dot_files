# global snippets

snippet     date_stamp
alias       ds
options     word
    `strftime("%Y/%m/%d(%a)")`

snippet     time_stamp
alias       ts
options     word
    `strftime("%Y/%m/%d %H:%M:%S")`

snippet     magic_comment_vim
alias       vim_magic_comment vim_magic magic ft_magic magic_ft
options     word
    vim: set filetype=${1}:

snippet     line
options     word
    --------------------------------------------------

# vim: set filetype=neosnippet:

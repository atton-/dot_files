# my ruby snippets

# shebang
snippet     shebang
alias       shebang
options     head
	#!/usr/bin/env ruby


# magic comment
snippet     magic_comment_vim
alias       magic_comment_vim
options     head
	# vim:set fileencoding=utf-8:

snippet     magic_comment_emacs
alias       magic_comment_emacs
options     head
	# -*- coding: utf-8 -*-


# utils
snippet     pry
alias       pry
options     head
	binding.pry
